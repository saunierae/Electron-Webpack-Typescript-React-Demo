{
  "name": "final",
  "version": "0.1.0",
  "description": "Application for React, Webpack, Typescript, Electron and Babel.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "electron",
    "starter",
    "boilerplate"
  ],
  "author": "Saunie Burke",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saunierae/School/SDEV435/Final.git"
  },
  "bugs": {
    "url": "https://github.com/saunierae/School/SDEV435/Final/Issues"
  },
  "main": "src/electron.tsx",
  "homepage": "./",
  "build": {
    "productName": "Electron Test",
    "files": [
      "build/**/*"
    ],
    "extraMetadata": {
      "main": "build/electron.tsx"
    }
  },
  "scripts": {
    "launch-electron": "node_modules/electron/dist/electron.exe path-to-app",
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rimraf dist/*",
    "lint": "tslint './src/**/*.ts*' --format stylish --project . --force",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "electron-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron.\"",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --coverage --watchAll --config=configs/jest.json",
    "electron-pack": "electron-builder"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/redux":"^3.6.0",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "electron": "^9.0.2",
    "express": "^4.17.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "image-webpack-loader": "^6.0.0",
    "jest": "^26.0.1",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "redux": "4.0.5",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "source-map-loader": "*",
    "style-loader": "^1.2.1",
    "tslint": "^6.1.2",
    "ts-loader": "7.0.5",
    "typescript": "^3.9.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
